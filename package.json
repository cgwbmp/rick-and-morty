{
  "name": "rick-and-morty",
  "version": "0.1.0",
  "private": true,
  "description": "The Rick and Morty Wiki",
  "author": "Alena <cgwbmp@gmail.com>",
  "homepage": "https://cgwbmp.github.io/rick-and-morty",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
